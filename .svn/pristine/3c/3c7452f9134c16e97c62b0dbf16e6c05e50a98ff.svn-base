#upload span,#choose span,#preprocess span,#analyse span,#other span{
	position: relative;
	top: 60px;
	font-family: '黑体';
	color:white;
	font-size: 14px;
}

#upload:hover span,#choose:hover span,#preprocess:hover span,#analyse:hover span,#other:hover span{
	color:rgb(3,82,151);
	cursor: pointer;
	-webkit-transition: color .25s linear; 
}

	